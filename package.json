{
  "name": "tuna",
  "version": "0.0.1",
  "description": "A cross-platform iTunes alternative",
  "main": "src/main.js",
  "author": "Jan Wirth <contact@jan-wirth.de>",
  "scripts": {
    "start": "nodemon --exec \"npm run elm && npm run electron\" -w src -e elm,js",
    "elm": "elm make --debug src/Main.elm --output elm-stuff/elm.js && npm run monkey-patch",
    "elm-optimize": "elm make --optimize src/Main.elm --output elm-stuff/elm.js && npm run monkey-patch",
    "monkey-patch": "sh monkey-patch.sh",
    "electron": "electron .",
    "build": "npm run elm-optimize && electron-builder"
  },
  "electronDownload": {
    "cache": "~/home/.cache/electron-builder"
  },
  "keywords": [
    "Electron"
  ],
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.0.3",
    "electron-builder": "^22.4.0"
  },
  "dependencies": {
    "better-queue": "^3.8.10",
    "concurrently": "^5.1.0",
    "decompress-zip": "^0.2.2",
    "electron-reload": "^1.5.0",
    "electron-root-path": "^1.0.16",
    "electron-serve": "^0.4.1",
    "elm": "^0.19.1-3",
    "hasha": "^5.2.0",
    "html-entities": "^1.2.1",
    "isomorphic-fetch": "^2.2.1",
    "music-metadata": "^6.3.6",
    "nodemon": "^2.0.2",
    "recursive-readdir": "^2.2.2",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "rimraf": "^3.0.2",
    "rimraf-promise": "^2.0.0"
  }
}
